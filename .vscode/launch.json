            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "${workspaceFolder}/data-bin/it",
                "--task", "translation",
                "--path", "${workspaceFolder}/pretrain-models/wmt19.de-en/wmt19.de-en.ffn8192.pt",
                "--dataset-impl", "mmap",
                "--beam", "4",
                "--lenpen", "0.6",
                "--max-len-a", "1.2",
